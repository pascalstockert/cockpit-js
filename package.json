{
  "name": "@pasu/cockpit-client",
  "version": "0.1.2",
  "description": "An API client for the headless CMS Cockpit",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "bump:patch": "npm version patch && @sync-git-tag",
    "bump:minor": "npm version minor && @sync-git-tag",
    "bump:major": "npm version major && @sync-git-tag",
    "sync-git-tag": "sh -c 'git tag v$(npm pkg get version | tr -d \\\"\\\")'"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pascalstockert/cockpit-js.git"
  },
  "keywords": [
    "api",
    "typescript",
    "client"
  ],
  "author": "Pascal Stockert",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "typescript": "^5.9.2"
  }
}
